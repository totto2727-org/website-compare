{
  "name": "website-compare",
  "type": "module",
  "scripts": {
    "dev": "vite --mode ssr",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "preview": "wrangler pages dev dist",
    "deploy": "run-z build deploy",
    "deploy:pages": "wrangler pages deploy dist",
    "check": "run-z check:tsc,check:biome",
    "check:tsc": "tsc",
    "check:biome": "biome check .",
    "fix": "run-z fix:biome",
    "fix:biome": "biome check --apply-unsafe .",
    "test": "run-z test:unit",
    "test:unit": "bun test"
  },
  "dependencies": {
    "@biomejs/biome": "^1.5.2",
    "@builder.io/qwik": "^1.4.3",
    "@builder.io/qwik-city": "^1.4.3",
    "@cloudflare/workers-types": "^4.20231218.0",
    "@totto2727/result": "^0.6.8",
    "bun-types": "latest",
    "hono": "^3.12.6",
    "remeda": "^1.37.0",
    "run-z": "^2.0.0",
    "typescript": "5.3.3",
    "undici": "^6.6.0",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "wrangler": "^3.22.0"
  }
}
